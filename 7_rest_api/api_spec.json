{
  "openapi": "3.0.2",
  "info": {
    "title": "Category RESTFull APi",
    "version": "1.0"
  },
  "servers": [
    {
      "url": "https://localhost:8000/api/v1"
    }
  ],
  "paths": {
    "/caterogies": {
      "get": {
        "description": "List all categories",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "code": {
                      "type": "number"
                    },
                    "status": {
                      "type": "string"
                    },
                    "data": {
                      "type": "array"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}